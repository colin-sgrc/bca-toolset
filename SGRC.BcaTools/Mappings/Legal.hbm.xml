<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SGRC.BCATools" namespace="SGRC.BCATools">

  <class name="Legal" table="Legal" lazy="true">
    <id name="Folio" column="folio">
      <generator class="assigned"/>
    </id>
    
    <property name="Jurisdiction" column="jurisdiction"/>
    <property name="Roll" column="roll"/>

    <property name="Area" column="area"/>
    <property name="Lot" column="lot"/>
    <property name="Block" column="block"/>
    <property name="Section" column="`section`"/>

    <property name="Township" column="township"/>
    <property name="PlanNumber" column="plan_no"/>
    <property name="DistrictLot" column="district_lot"/>

    <property name="LandDistrict" column="land_district"/>
    <property name="LotSizeCode" column="lot_size_code"/>


    <property name="AreaUnit" column="area_unit"/>
    <property name="Width" column="width"/>
    <property name="Depth" column="depth"/>
    <property name="LotArea" column="lot_area"/>
    <property name="StreetNumber" column="street_no"/>
    <property name="AptNumber" column="apt_no"/>
    <property name="StreetDirection" column="street_direction"/>
    <property name="StreetName" column="street_name"/>
    <property name="PID" column="pid"/>
    <property name="Description1" column="description1"/>
    <property name="Description2" column="description2"/>
    <property name="Description3" column="description3"/>
    <property name="Description4" column="description4"/>
    <property name="Description5" column="description5"/>
    <property name="Description6" column="description6"/>
    <property name="Description7" column="description7"/>
    <property name="Description8" column="description8"/>
    <property name="Description9" column="description9"/>
  </class>

</hibernate-mapping>