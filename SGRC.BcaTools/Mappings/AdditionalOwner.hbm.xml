<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SGRC.BCATools" namespace="SGRC.BCATools">

  <class name="AdditionalOwner" table="Extended_Owners" lazy="true">
    <id name="FolioGroup" column="folio_group">
      <generator class="assigned"/>
    </id>

    <property name="Folio" column="folio"/>
    <property name="GroupCode" column="group_code"/>

    <property name="Jurisdiction" column="jurisdiction"/>
    <property name="Roll" column="roll"/>

    <property name="Surname1" column="sur_name1"/>
    <property name="FirstName1" column="first_name1"/>
    <property name="Initial1" column="initial1"/>

    <property name="Surname2" column="sur_name2"/>
    <property name="FirstName2" column="first_name2"/>
    <property name="Initial2" column="initial2"/>

    <property name="PostalCode" column="postal_code"/>
    
    <property name="Address1" column="address1"/>
    <property name="Address2" column="address2"/>
    <property name="Address3" column="address3"/>
    <property name="Address4" column="address4"/>
    <property name="Address5" column="address5"/>
    <property name="Address6" column="address6"/>
  </class>

</hibernate-mapping>